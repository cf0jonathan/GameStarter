<?xml version="1.0" encoding="utf-8"?>

<Level>
  <!-- Player GameObject with movement controls -->
  <GameObject id="player">
    <BodyComponent x="100" y="200" vx="0" vy="0" angle="0" />
    <SpriteComponent textureKey="squirrel" />
    <ControllerComponent speed="5.0" />
  </GameObject>
  
  <!-- Sliding enemy -->
  <GameObject id="slider">
    <BodyComponent x="400" y="100" vx="0" vy="0" angle="0" />
    <SpriteComponent textureKey="squirrel" />
    <SlideComponent speed="3.0" leftBound="200" rightBound="600" />
  </GameObject>
  
  <!-- Spinning object -->
  <GameObject id="spinner">
    <BodyComponent x="400" y="400" vx="0" vy="0" angle="0" />
    <SpriteComponent textureKey="squirrel" />
    <RotateComponent rotationSpeed="2.0" />
  </GameObject>
  
  <!-- Combo object that slides and spins -->
  <GameObject id="combo">
    <BodyComponent x="100" y="500" vx="0" vy="0" angle="0" />
    <SpriteComponent textureKey="squirrel" />
    <SlideComponent speed="2.0" leftBound="50" rightBound="750" />
    <RotateComponent rotationSpeed="5.0" />
  </GameObject>
  
  <!-- Static decorative objects -->
  <GameObject id="decoration1">
    <BodyComponent x="600" y="300" vx="0" vy="0" angle="45" />
    <SpriteComponent textureKey="squirrel" />
  </GameObject>
  
  <GameObject id="decoration2">
    <BodyComponent x="200" y="350" vx="0" vy="0" angle="90" />
    <SpriteComponent textureKey="squirrel" />
  </GameObject>
</Level>